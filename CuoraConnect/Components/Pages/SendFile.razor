    private bool TableExists(string tableName)
    {
        var _pathDB = FileUploadService.pathDB();

        // Conecta ao banco de dados
        using var db = new SQLiteConnection(_pathDB);

        // Executa uma consulta para verificar se a tabela existe no esquema
        var query = $"SELECT name FROM sqlite_master WHERE type='table' AND name='{tableName}';";

        var result = db.ExecuteScalar<string>(query);

        // Se result não for nulo ou vazio, a tabela existe
        return !string.IsNullOrEmpty(result);
    }

    private async Task EnviarConfig()
    {
        string filePath = FileUploadService.SaveXmlToFile();

        // Verifica se o arquivo existe
        if (!File.Exists(filePath))
        {
            statusMessage = "Arquivo não encontrado.";
            StateHasChanged();
            return;
        }

        try
        {
            var response = await BasicAuthWithFile();
        }
        catch (Exception ex) // Captura qualquer exceção
        {
            statusMessage = $"Erro ao enviar configuração: {ex.Message}";
            StateHasChanged();
            // Aqui você pode adicionar lógica adicional para registrar o erro ou exibi-lo ao usuário
        }

        async Task<HttpWebResponse> BasicAuthWithFile()
        {
            var url = new Uri("http://192.168.0.1/import/config");
            var request = (HttpWebRequest)WebRequest.Create(url);
            request.Method = "POST";
            var authValue = Convert.ToBase64String(Encoding.ASCII.GetBytes("admin:admin"));
            request.Headers["Authorization"] = "Basic " + authValue;

            var boundary = "===" + DateTime.Now.Ticks.ToString("x") + "===";
            request.ContentType = $"multipart/form-data; boundary={boundary}";

            using (var requestStream = await request.GetRequestStreamAsync())
            using (var writer = new StreamWriter(requestStream, Encoding.UTF8))
            {
                var fileName = Path.GetFileName(filePath);
                writer.Write($"--{boundary}\r\n");
                writer.Write($"Content-Disposition: form-data; name=\"configrecord\"; filename=\"{fileName}\"\r\n");
                writer.Write($"Content-Type: application/xml\r\n\r\n");
                writer.Flush();

                using (var fileStream = File.OpenRead(filePath))
                {
                    await fileStream.CopyToAsync(requestStream);
                }

                writer.Write($"\r\n--{boundary}--\r\n");
                writer.Flush();
            }

            try
            {
                var response = (HttpWebResponse)await request.GetResponseAsync();
                // Lê o corpo da resposta
                using (var streamReader = new StreamReader(response.GetResponseStream()))
                {
                    string responseBody = streamReader.ReadToEnd();
                    statusMessage = $"Resposta do servidor: {responseBody}";
                    StateHasChanged();
                }

                return response;
            }
            catch (WebException ex)
            {
                using (var responseStream = ex.Response.GetResponseStream())
                using (var reader = new StreamReader(responseStream))
                {
                    string errorResponse = await reader.ReadToEndAsync();


                    // Verifica se a mensagem de erro contém o código de status 401
                    if (ex.Response is HttpWebResponse httpResponse && httpResponse.StatusCode == HttpStatusCode.Unauthorized)
                    {
                        // Chama o método desejado se a resposta for 401
                        SendConfigBasicAuth(); // Substitua pelo nome do seu método

                    }
                    else
                    {
                        
                        statusMessage = $"Erro: {ex.Message}, Resposta: {errorResponse}";
                        StateHasChanged();
                    }
                }
                // Não lança a exceção novamente
                return null; // Ou outra lógica que você preferir
            }
        }
    }




